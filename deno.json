{
  "lock": false,
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "esm:add": "deno run -A https://esm.sh/v129 add",
    "esm:update": "deno run -A https://esm.sh/v129 update",
    "esm:remove": "deno run -A https://esm.sh/v129 remove",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    },
    "exclude": [
      "_fresh"
    ]
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "$std/": "https://deno.land/std@0.193.0/",
    "$fresh/": "https://deno.land/x/fresh@1.4.3/",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "preact": "https://esm.sh/preact@10.15.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/",
    "@twind/core": "https://esm.sh/v129/*@twind/core@1.1.3",
    "@twind/preset-autoprefix": "https://esm.sh/v129/*@twind/preset-autoprefix@1.0.7",
    "@twind/preset-container-queries": "https://esm.sh/v129/*@twind/preset-container-queries@1.0.7",
    "@twind/preset-tailwind": "https://esm.sh/v129/*@twind/preset-tailwind@1.1.4",
    "@twind/preset-tailwind/": "https://esm.sh/v129/*@twind/preset-tailwind@1.1.4/",
    "@twind/preset-typography": "https://esm.sh/v129/*@twind/preset-typography@1.0.7"
  },
  "scopes": {
    "https://esm.sh/v129/": {
      "csstype": "https://esm.sh/v129/csstype@3.1.2",
      "style-vendorizer": "https://esm.sh/v129/style-vendorizer@2.2.3"
    }
  },
  "fmt": {
    "exclude": [
      "_fresh"
    ]
  }
}